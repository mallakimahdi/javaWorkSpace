package spring;

public class sortBinary 
{
	public static void main(String[] args)
	{
		int[] A,B,C;
		A = new int[30];
		B = new int[50];
		C = new int[50];
		
		for(int i=0; i<C.length ; i++)
		{
			C[i] = 0;
		}
		for(int j=0 ; j<A.length ; j++)
		{
			C[A[j]]++;
		}
		for(int i=0; i<C.length; i++)
		{
			C[i] = C[i] + C[i-1];
		}
		for(int j=A.length ; j>0 ; j--)
		{
			B[C[A[j]]] = A[j];
			
		}
	}
}